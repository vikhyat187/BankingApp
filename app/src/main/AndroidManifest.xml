<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sleepingpandaaa.bankingsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label= "@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BankingSystem">
        <activity android:name=".MainActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Userlist"
            android:screenOrientation="portrait"
            android:label="Users List"
            ></activity>
        <activity
            android:name=".Userdata"
            android:screenOrientation="portrait"
            android:label="User details"
            ></activity>
        <activity
            android:name=".SendToUser"
            android:screenOrientation="portrait"
            android:label="Transfer money"

            ></activity>
        <activity
            android:name=".HistoryList"
            android:parentActivityName=".Userlist"
            android:screenOrientation="portrait"
            android:label="Transactions"
            />
    </application>

</manifest>